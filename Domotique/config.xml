<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE simulation>
<simulation name="Phase One test simulation">
	<process name="Temperature">
		<phenomenon name="External Temperature" type="Random">
			<valueLow>-100</valueLow>
			<valueHigh>400</valueHigh>
		</phenomenon>
		<controller name="Heater/cooler" type="Threshold">
			<saturation>201</saturation>
		</controller>
		<state name="Room" type="State">
			<influencePhenomenon>0.01</influencePhenomenon>
			<influenceController>0.09</influenceController>
		</state>
	</process>
	<runner>
		<ticks>30</ticks>
	</runner>
	<process name="Flooding">
		<phenomenon name="Water height" type="Random">
			<valueLow>0</valueLow>
			<valueHigh>450</valueHigh>
		</phenomenon>
		<controller name="Lock" type="Threshold">
			<saturation>5</saturation>
		</controller>
		<state name="Canal" type="State">
			<influencePhenomenon>0.001</influencePhenomenon>
			<influenceController>0.9</influenceController>
		</state>
	</process>

</simulation>